{% extends 'base.html.twig' %}

{% block title %}Hello HomeController!{% endblock %}

{% block body %}

    <div class="container">
        <div class="row">
            <div class="form-group col-md-6">
                <label for="sel1">Company Symbol:</label>
                <select class="form-control" name="company_symbol" id="companySymbol" required>
                    <option>Select Company Symbol</option>
                    <option>1</option>
                </select>
            </div>
            <div class="form-group col-md-6">
                <label for="usr">Email:</label>
                <input type="email" placeholder="Example@example.com" class="form-control" name="email" id="email" required>
            </div>
        </div>
        <div class="row">
            <div class="form-group col-md-6">
                <label for="pwd">Start Date:</label>
                <input type="date" class="form-control" name="start_date" id="startDate" required>
            </div>
            <div class="form-group col-md-6">
                <label for="pwd">End Date:</label>
                <input type="date" class="form-control" name="end_date" id="endDate" required>
            </div>
        </div>
        <button class="btn btn-success" id="processButton">Process</button>
    </div>

{% endblock %}
